@using SN.Models;
@{
    ViewBag.Title = "Add photo";
}

    <link rel="stylesheet" href="~/css/bootstrap.css">
    <script src="~/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/rx.js")" type="text/javascript"></script> 
    <script src="@Url.Content("~/Scripts/rx.jquery.js")" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="~/Scripts/mdd_styles.css" />
    <script type="text/javascript" src="~/Scripts/jQuery-1.7.1.min.js"></script>

    <head>
      <title>Add photo to my photo gallery</title>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width">
    </head>

    <div id="dialog" title="Upload files"> 
    @using (Html.BeginForm("AddPhoto", "Profile", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <h2>Files uploaded to server</h2>    
                <p><input type="file" id="fileUpload" name="fileUpload"/></p>
                <p><input type="submit" value="Upload file" /></p>            
    }
    </div>